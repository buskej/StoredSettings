[global_config]
  title_transmit_fg_color = "#000000"
  title_transmit_bg_color = "#deddda"
  title_receive_fg_color = "#000000"
  title_receive_bg_color = "#8ff0a4"
  title_inactive_bg_color = "#000000"
  inactive_color_offset = 0.7366071428571429
  line_height = 1.1
[keybindings]
  copy = <Primary>c
  paste = <Primary>v
  close_window = <Primary>q
[profiles]
  [[default]]
    visible_bell = True
    background_darkness = 0.8
    background_type = transparent
    cursor_shape = ibeam
    cursor_color = "#aaaaaa"
    foreground_color = "#ffffff"
    scrollbar_position = hidden
    scrollback_lines = 1000
[layouts]
  [[default]]
    [[[window0]]]
      type = Window
      parent = ""
    [[[child1]]]
      type = Terminal
      parent = window0
[plugins]
